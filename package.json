{
  "name": "apartment-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma migrate deploy && prisma generate && next build",
    "start": "next start",
    "lint": "eslint src",
    "typecheck:base": "tsc -p tsconfig.base.json --noEmit",
    "typecheck:app": "tsc -p tsconfig.app.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init_core",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "seed": "prisma db seed",
    "outbox:process": "tsx --tsconfig tsconfig.scripts.json scripts/processTicketOutbox.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "next": "^16.1.4",
    "next-auth": "^4.24.13",
    "pdf-lib": "^1.17.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prisma": "^5.20.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.17"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
